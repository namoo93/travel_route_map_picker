{
  "tabWidth": 1,
  "useTabs": true,
  "endOfLine": "auto",
  "singleQuote": true,
  "printWidth": 120,
  "trailingComma": "all",
  "bracketSpacing": false,
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "semi": true,
  "vueIndentScriptAndStyle": true,
  "requirePragma": false,
  "insertPragma": false,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "printWidth": 200
      }
    }
  ]
}
